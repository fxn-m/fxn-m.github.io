{
  "homepage": "https://fxn-m.com/",
  "name": "fxn-m.com",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.8.0",
  "scripts": {
    "build": "pnpm build:voltaire && pnpm build:markdown && run-p type-check build-only",
    "build:markdown": "npx tsx --tsconfig tsconfig.server.json ./scripts/buildBlog.ts",
    "build-only": "vite build",
    "preview": "vite preview",
    "dev": "vite",
    "dev:all": "run-p dev voltaire:dev",
    "dev:server": "wrangler dev",
    "lint": "npx eslint --ext .ts,.js,.vue src",
    "format": "prettier . --write",
    "type-check": "run-s type-check:app type-check:server",
    "type-check:app": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "type-check:server": "tsc --noEmit -p tsconfig.server.json",
    "tunnel:server": "ngrok http --url=discrete-allegedly-eagle.ngrok-free.app localhost:8787",
    "tunnel:client": "ngrok http --url=discrete-allegedly-eagle.ngrok-free.app localhost:5173",
    "build:voltaire": "run-s voltaire:build voltaire:copy",
    "voltaire:build": "pnpm --filter @fxn/voltaire build",
    "voltaire:watch": "pnpm --filter @fxn/voltaire watch",
    "voltaire:dev": "cross-env VITE_VOLTAIRE_DEBUG=1 pnpm --filter @fxn/voltaire dev",
    "voltaire:copy": "node ./scripts/syncVoltaire.mjs"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.53",
    "@ai-sdk/vue": "^2.0.76",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@heroicons/react": "^2.2.0",
    "@heroicons/vue": "^2.2.0",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@mapbox/polyline": "^1.2.1",
    "@notionhq/client": "^5.1.0",
    "@tanstack/form-core": "^1.24.4",
    "@tanstack/vue-form": "^1.23.8",
    "@tanstack/vue-query": "^5.90.2",
    "@tanstack/vue-table": "^8.21.3",
    "@trpc/client": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@vueuse/core": "^13.9.0",
    "ai": "^5.0.76",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "embla-carousel-vue": "^8.6.0",
    "he": "^1.2.0",
    "highlight.js": "^11.11.1",
    "js-confetti": "^0.13.1",
    "lucide-vue-next": "^0.511.0",
    "motion-v": "^1.7.3",
    "notion-to-md": "4.0.0-alpha.4",
    "p-limit": "^6.2.0",
    "posthog-js": "^1.279.0",
    "radix-vue": "^1.9.17",
    "reka-ui": "^2.6.0",
    "showdown": "^2.1.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "uuid": "^11.1.0",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/css": "^0.12.0",
    "@eslint/js": "^9.37.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.4.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/vite": "^4.1.14",
    "@tsconfig/node18": "^18.2.4",
    "@types/he": "^1.2.3",
    "@types/mapbox__polyline": "^1.0.5",
    "@types/node": "^18.19.129",
    "@types/showdown": "^2.0.6",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.37.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.20.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^6.3.6",
    "vue-tsc": "^3.1.1",
    "wrangler": "^4.45.3"
  }
}
